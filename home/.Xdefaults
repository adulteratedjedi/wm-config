!{{{ XFT
Xft.dpi:       96
Xft.antialias: 1
Xft.hinting:   1
Xft.hintstyle: hintfull
Xft.rgba:      rgb
!}}}

!{{{ jwr low contrast

!black
*color0: #222222
*color8: #454545

!red
*color1: #9E5641
*color9: #CC896D
!green
*color2: #6C7E55
*color10: #C4DF90
!yellow
*color3: #CAAF2B
*color11: #FFE080
!blue
*color4: #7FB8D8
*color12: #B8DDEA
!magenta
*color5: #956D9D
*color13: #C18FCB
!cyan
*color6: #4c8ea1
*color14: #6bc1d0
!white
*color7: #808080
*color15: #cdcdcd

*borderColor: #343434
*background: #343434
*foreground: #cdcdcd
!}}}

!{{{ rxvt-unicode ---------------------------------------------------------------

URxvt*font:	xft:Liberation Mono:pixelsize=16,\
			xft:Arial Unicode MS:pixelsize=16,\
			xft:Everson Mono Unicode:pixelsize=16

urxvt*saveLines:  32767
urxvt*visualBell: false
urxvt*loginShell: true
URxvt*termName: 	rxvt-256

URxvt*searchable-scrollback: CS-s
URxvt.cutchars: "()*,<>[]{}|'
URxvt*secondaryScroll: true
URxvt.urgentOnBell: true
!}}}





